{
    "name": "tightenco/overload",
    "description": "Method overloading for PHP",
    "keywords": [
        "tighten",
        "overload",
        "overloadable",
        "php"
    ],
    "homepage": "https://github.com/tighten/overload",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Wathan",
            "email": "adam.wathan@gmail.com",
            "homepage": "https://adamwathan.com",
            "role": "Developer"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co",
            "homepage": "https://tighten.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "tightenco/collect": "^5.6 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Overload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\Overload\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
